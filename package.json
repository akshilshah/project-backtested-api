{
  "name": "backtested-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "export PORT=31003 && pm2 start run.js --name backtested-api",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "db:generate": "yarn prisma generate",
    "db:migrate": "yarn prisma migrate dev",
    "db:migrate:apply": "yarn prisma migrate deploy",
    "studio": "yarn prisma studio",
    "build": "NODE_ENV=build rollup -c",
    "build-prod": "NODE_ENV=production rollup -c",
    "restart": "pm2 restart run.js --name backtested-api",
    "stop": "pm2 delete backtested-api",
    "build:magic": "NODE_ENV=build rollup -c rollup.magic.config.js",
    "magic": " rollup -c rollup.magic.config.js -w --environment NODE_ENV:development",
    "update:prisma": "yarn add prisma @prisma/client",
    "u:p": "yarn add prisma @prisma/client",
    "release": "node release/index.js",
    "flush:cache": "redis-cli --scan --pattern 'afg:*' | while IFS= read -r k; do redis-cli UNLINK \"$k\"; done"
  },
